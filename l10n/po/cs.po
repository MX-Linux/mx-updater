#. msgstr[1] -> plural form: use '{num} new updates'.
#, python-brace-format
msgid "You have {num} new update available"
msgid_plural "You have {num} new updates available"
msgstr[0] "Je k dispozici {num} nová aktualizace"
msgstr[1] "Je k dispozici {num} nové aktualizace"
msgstr[2] "Je k dispozici {num} nových aktualizací"
msgstr[3] "Je k dispozici {num} nových aktualizací"

msgid "automatically close terminal window when full/basic upgrade complete"
msgstr ""
"automaticky zavřít terminálové okno jakmile je úplná/základní aktualizace "
"dokončena"

msgid "automatically close terminal window when upgrade complete"
msgstr "automaticky zavřít terminálové okno jakmile je aktualizace dokončena"

msgid "basic upgrade"
msgstr "základní aktualizace"

msgid "basic upgrade complete (or was canceled)"
msgstr "základní aktualizace dokončena (nebo přerušena)"

msgid "check for autoremovable packages after full/basic upgrade"
msgstr ""
"kontrola automaticky odebratelných balíčků po úplné/základní aktualizaci"

msgid "classic"
msgstr "klasicky"

msgid "full upgrade"
msgstr "úplná aktualizace"

msgid "full upgrade   (recommended)"
msgstr "úplná aktualizace (doporučeno)"

msgid "full upgrade complete (or was canceled)"
msgstr "úplná aktualizace dokončena (nebo přerušena)"

msgid "opens MX Updater 'View and Upgrade' window"
msgstr "otevře okno MX Aktualizace 'Zobrazit a aktualizovat'"

msgid "opens Synaptic"
msgstr "spouští Synaptic"

msgid "press any key to close"
msgstr "Pro ukončení stiskněte libovolnou klávesu"

msgid "press any key to stop automatic closing"
msgstr "Stiskněte libovolnou klávesu pro zrušení automatického zavření"

msgid "pulse"
msgstr "puls"

msgid "start MX Updater at login"
msgstr "Spustit MX Aktualizace při přihlášení"

msgid "switch to basic upgrade"
msgstr "přepnout na základní aktualizaci"

msgid "switch to full upgrade"
msgstr "přepnout na úplnou aktualizaci"

msgid "this terminal window can now be closed"
msgstr "toto okno terminálu už můžete zavřít"

msgid "update automatically   (will not add new or remove existing packages)"
msgstr ""
"aktualizovat automaticky (nebude přidávat nové, ani odebírat stávající "
"balíčky)"

msgid "upgrade"
msgstr "aktualizace"

msgid "use desktop notifications"
msgstr "používat notifikace"

msgid "use transparent interior for no-updates wireframe"
msgstr "pouźít průhlednou ikonu u statusu Plně aktualizováno"

msgid "wireframe"
msgstr "drátový model"

