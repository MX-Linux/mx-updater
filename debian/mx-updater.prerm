#!/bin/bash

#DEBHELPER#

# prerm script for mx-updater

set -e

case "$1" in
  purge|remove)  
          rm -r /usr/lib/mx-updater/modules/__pycache__  2>/dev/null  || :      
          rm -r /usr/libexec/mx-updater/__pycache__  2>/dev/null  || :      
          ;;
          *)  :
          ;;  
esac
exit 0
