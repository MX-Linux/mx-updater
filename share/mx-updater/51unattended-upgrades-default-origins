// file  /etc/apt/apt.conf.d/51unattended-upgrades-defaults-origins
// part of mx-updater package
//
// Note: Do not edit this file.
// This file will be overwritten during an upgrade with the default settings.
//
Unattended-Upgrade::Origins-Pattern {
//
// Patterns below will be added to those 50unattended-upgrades enables by default
//
// Debian repos not enabled by default in 50unattended-upgrades
    "origin=Debian,codename=${distro_codename}-updates";
//  "origin=Debian,codename=${distro_codename}-proposed-updates";
//
// MX repos allowed to have unattended upgrades
    "o=MX repository,a=mx,n=${distro_codename},l=MX repository,c=ahs";
    "o=MX repository,a=mx,n=${distro_codename},l=MX repository,c=main";
    "o=MX repository,a=mx,n=${distro_codename},l=MX repository,c=non-free";
//  "o=MX repository,a=mx,n=${distro_codename},l=MX repository,c=non-free-firmware";
};
